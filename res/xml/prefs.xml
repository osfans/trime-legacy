<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    android:title="@string/ime_name">

    <CheckBoxPreference
        android:key="@string/prefs_vibrate_key"
        android:title="@string/prefs_vibrate_title"
        android:persistent="true"/>

    <CheckBoxPreference
        android:key="@string/prefs_sound_key"
        android:title="@string/prefs_sound_title"
        android:persistent="true"/>

    <CheckBoxPreference
        android:key="@string/prefs_sc_key"
        android:title="@string/prefs_sc_title"
        android:persistent="true"/>

    <CheckBoxPreference
        android:key="@string/prefs_commit_py_key"
        android:title="@string/prefs_commit_py_title"
        android:summary="@string/prefs_commit_py_summary"
        android:persistent="true"/>

    <CheckBoxPreference
        android:key="@string/prefs_full_py_key"
        android:title="@string/prefs_full_py_title"
        android:summary="@string/prefs_full_py_summary"
        android:persistent="true"/>

    <CheckBoxPreference
        android:key="@string/prefs_shape_code_key"
        android:title="@string/prefs_shape_code_title"
        android:summary="@string/prefs_shape_code_summary"
        android:persistent="true"/>

    <CheckBoxPreference
        android:key="@string/prefs_keyboard_preview_key"
        android:title="@string/prefs_keyboard_preview_title"
        android:summary="@string/prefs_keyboard_preview_summary"
        android:persistent="true"/>

    <Preference
        android:key="@string/prefs_importdb"
        android:title="@string/prefs_importdb"
        android:summary="@string/prefs_importdb_summary"
        android:persistent="false"/>

    <Preference
        android:key="@string/prefs_exportdb"
        android:title="@string/prefs_exportdb"
        android:summary="@string/prefs_exportdb_summary"
        android:persistent="false"/>

    <Preference
        android:key="@string/prefs_licensing"
        android:title="@string/prefs_licensing"
        android:persistent="false"/>

</PreferenceScreen>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="14%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height_popup"
    >

    <Row android:rowEdgeFlags="top">
        <Key android:keyLabel=":)" android:keyOutputText=" :)" android:keyEdgeFlags="left" />
        <Key android:keyLabel=":(" android:keyOutputText=" :(" />
        <Key android:keyLabel=";)" android:keyOutputText=" ;)" />
        <Key android:keyLabel=":'(" android:keyOutputText=" :'(" />
        <Key android:keyLabel="B-)" android:keyOutputText=" B-)" android:keyEdgeFlags="right" />
    </Row>

    <Row>
        <Key android:keyLabel=":P" android:keyOutputText=" :P" android:keyEdgeFlags="left" />
        <Key android:keyLabel=":O" android:keyOutputText=" :O" />
        <Key android:keyLabel=":D" android:keyOutputText=" :D" />
        <Key android:keyLabel=":S" android:keyOutputText=" :S" />
        <Key android:keyLabel="XD" android:keyOutputText=" XD" android:keyEdgeFlags="right" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key android:keyLabel="T_T" android:keyOutputText=" T_T" android:keyEdgeFlags="left" />
        <Key android:keyLabel="O.o" android:keyOutputText=" O.o" />
        <Key android:keyLabel="-.-" android:keyOutputText=" -.-" />
        <Key android:keyLabel="Orz" android:keyOutputText=" Orz" />
        <Key android:keyLabel="囧" android:keyOutputText=" 囧" android:keyEdgeFlags="right" />
    </Row>

</Keyboard>
